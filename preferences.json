[
  {
    "type": "separator",
    "label": "Feature Toggles",
    "id": "toggles-seperator"
  },
  {
    "property": "extensions.tabgroups.enable_sort",
    "label": "Enable the sort tabs button.",
    "type": "checkbox",
    "defaultValue": true
    },
    {
    "property": "extensions.tabgroups.enable_clear",
    "label": "Enable the clear tabs button.",
    "type": "checkbox",
    "defaultValue": true
    },
  {
    "property": "extensions.tabgroups.ai_model",
    "label": "Choose AI - Gemini, Ollama, or Mistral",
    "type": "dropdown",
    "placeholder": false,
    "value": "number",
    "default": 1,
    "options": [
      {
        "value": 1,
        "label": "Gemini"
      },
      {
        "value": 2,
        "label": "Ollama"
      },
      {
        "value": 3,
        "label": "Mistral"
      }
    ],
    "conditions": [
      { "if": { "property": "extensions.tabgroups.enable_sort", "value":true}}
    ]
  },
  {
    "type": "separator",
    "label": "Gemini",
    "id": "gemini-separator",
    "conditions": [
      { "if": { "property": "extensions.tabgroups.ai_model", "value": 1 }}, 
      { "if": { "property": "extensions.tabgroups.enable_sort", "value":true}}
    ],
    "operator": "AND"
  },
  {
    "property": "extensions.tabgroups.gemini_api_key",
    "label": "Gemini API Key",
    "type": "string",
    "placeholder": "Enter your Gemini API key here",
    "conditions": [
      { "if": { "property": "extensions.tabgroups.ai_model", "value": 1 } }, 
      { "if": { "property": "extensions.tabgroups.enable_sort", "value":true}}
    ],
    "operator": "AND"
  },
  {
    "property": "extensions.tabgroups.gemini_model",
    "label": "Gemini Model to Use For Renaming",
    "default": "gemini-2.0-flash",
    "type": "string",
    "placeholder": "Enter your Gemini Model here",
    "conditions": [
      { "if": { "property": "extensions.tabgroups.ai_model", "value": 1 } }, 
      { "if": { "property": "extensions.tabgroups.enable_sort", "value":true}}
    ],
    "operator": "AND"
  },
  {
    "type": "separator",
    "label": "Ollama",
    "id": "ollama-separator",
    "conditions": [
      { "if": { "property": "extensions.tabgroups.ai_model", "value": 2 } }, 
      { "if": { "property": "extensions.tabgroups.enable_sort", "value":true}}
    ],
    "operator": "AND"
  },
  {
    "property": "extensions.tabgroups.ollama_endpoint",
    "label": "Ollama Endpoint URL",
    "default": "http://localhost:11434/api/generate",
    "type": "string",
    "placeholder": "Enter your Ollama Endpoint URL here",
    "conditions": [
      { "if": { "property": "extensions.tabgroups.ai_model", "value": 2 } }, 
      { "if": { "property": "extensions.tabgroups.enable_sort", "value":true}}
    ],
    "operator": "AND"
  },
  {
    "property": "extensions.tabgroups.ollama_model",
    "label": "Ollama Model to Use For Renaming",
    "default": "llama3.1:latest",
    "type": "string",
    "placeholder": "Enter your Ollama Model here",
    "conditions": [
      { "if": { "property": "extensions.tabgroups.ai_model", "value": 2 } }, 
      { "if": { "property": "extensions.tabgroups.enable_sort", "value":true}}
    ],
    "operator": "AND"
  },
  {
    "type": "separator",
    "label": "Mistral",
    "id": "mistral-separator",
    "conditions": [
      { "if": { "property": "extensions.tabgroups.ai_model", "value": 3 } }, 
      { "if": { "property": "extensions.tabgroups.enable_sort", "value":true}}
    ],
    "operator": "AND"
  },
  {
    "property": "extensions.tabgroups.mistral_api_key",
    "label": "Mistral API Key",
    "type": "string",
    "placeholder": "Enter your Mistral API key here",
    "conditions": [
      { "if": { "property": "extensions.tabgroups.ai_model", "value": 3 } }, 
      { "if": { "property": "extensions.tabgroups.enable_sort", "value":true}}
    ],
    "operator": "AND"
  },
  {
    "property": "extensions.tabgroups.mistral_model",
    "label": "Mistral Model to Use For Renaming",
    "default": "mistral-large-latest",
    "type": "string",
    "placeholder": "Enter your Mistral Model here",
    "conditions": [
      { "if": { "property": "extensions.tabgroups.ai_model", "value": 3 } }, 
      { "if": { "property": "extensions.tabgroups.enable_sort", "value":true}}
    ],
    "operator": "AND"
  }
]
